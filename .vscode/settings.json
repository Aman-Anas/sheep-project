{
    // File nesting for godot uid files
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "*": "$(capture).import, $(capture).uid"
    },
    "dotrush.roslyn.projectOrSolutionFiles": [
        "SheepSimulator.sln",
        "SheepSimulator.csproj"
    ],
    "dotrush.debugger.projectAssembliesOnly": false,
    "workbench.editorAssociations": {
        "*.speedscope.json": "speedscope-in-vscode.speedscope"
    }
}